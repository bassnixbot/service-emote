{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",  // This matches the "preLaunchTask" in launch.json
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/EmoteService.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
